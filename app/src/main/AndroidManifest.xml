<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.group13.mytask">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ProfilesActivitySettings" android:label="Profile Settings" />
        <activity android:name=".TriggerActivitySettings" android:label="Trigger Settings" />
        <activity android:name=".ProfilesActivity" android:label="Profiles" />
        <activity android:name=".TriggerActivity" android:label="Triggers"></activity>
    </application>

</manifest>